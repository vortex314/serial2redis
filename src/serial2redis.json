{
    "redis": {
        "host": "localhost",
        "port": 6379,
        "addReplyContext": true,
        "reconnectOnConnectionLoss": true,
        "ignoreReplies": [
            "publish"
        ],
        "initCommands": [
            "CLIENT REPLY OFF",
            "CLIENT SETNAME USB0",
            "CLIENT REPLY ON"
        ]
    },
    "serial": {
        "port": "/dev/ttyUSB0",
        "baudrate": 921600,
        "frame": "ppp"
    },
    "proxy": {
        "timeout": 5000
    },
    "log": {
        "prefix": "../log/serial2redis_",
        "count": 10,
        "limit": 1048576
    }
}
