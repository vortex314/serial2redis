{
    "redis": {
        "host": "localhost",
        "port": 6379,
        "addReplyContext": true,
        "reconnectOnConnectionLoss": true,
        "ignoreReplies": [
            "publish"
        ],
        "initCommands": [
            "CLIENT REPLY OFF",
            "CLIENT SETNAME USB0",
            "CLIENT REPLY ON"
        ]
    },
    "serial": {
        "port": "/dev/ttyACM0",
        "baudrate": 115200,
        "frame": "ppp",
        "format": "cbor"
    },
    "proxy": {
        "timeout": 5000
    },
    "log-ignore": {
        "prefix": "/home/lieven/log/serial2redis_",
        "count": 10,
        "limit": 1048576
    }
}
