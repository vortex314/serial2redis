{
    "redis": {
        "host": "localhost",
        "port": 6379,
        "addReplyContext": true,
        "reconnectOnConnectionLoss": true
    },
    "serial": {
        "port": "/dev/ttyUSB0",
        "baudrate": 115200,
        "frame": "ppp"
    },
    "proxy": {
        "timeout": 5000,
        "log": {
            "level": "warn",
            "file": "./proxy.log"
        },
        "commands": [
            "HELLO 3",
            "CLIENT SETNAME USB0",
            "CLIENT REPLY OFF",
            "PUBLISH src/redis/proxy $config"
        ]
    }
}