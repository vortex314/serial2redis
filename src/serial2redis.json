{
    "redis": {
        "host": "pcthink.local",
        "port": 6379,
        "addReplyContext": true,
        "reconnectOnConnectionLoss": true,
        "ignoreReplies": [
            "publish"
        ],
        "initCommands": [
            "hello 3"
        ]
    },
    "serial": {
        "port": "/dev/ttyUSB0",
        "baudrate": 115200,
        "frame": "crlf",
        "format": "json"
    },
    "proxy": {
        "timeout": 5000
    },
    "log2": {
        "prefix": "/home/lieven/log/USB0_",
        "count": 10,
        "limit": 1048576
    }
}